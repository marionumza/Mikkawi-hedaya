<odoo>
	<!-- Sequence configuration for the auto generation of the Quote Id number with a specified format-->
	<data noupdate="1">
		<record id="pos_quote_sequence" model="ir.sequence">
		<field name="name">quote_id</field>
		<field name="code">pos.quote</field>
		<field name="prefix">QUOTE</field>
		<field name="padding">3</field>
		</record>
	</data>
</odoo>
